	<script>
		var eth=0;
		function getTotalBalance(){
			var xmlhttp = new XMLHttpRequest();
			var address_eth_ico="0xF43dB4c9716c68D295CA9a625eC2302A16D52459";
			var url = "https://api.etherscan.io/api?module=account&action=balance&address="+address_eth_ico+"&tag=latest&apikey=T8NVZH5T23EMVZ4DZXUBGXJ3ZGGWMYI4Y4";
			
			xmlhttp.onreadystatechange = function() {
			if (this.readyState == 4 && this.status == 200) {
				var myArr = JSON.parse(this.responseText);
					var rs=myArr.result;
					rs=rs/1000000000000000000;
					eth=rs;
					$("#total_eth").html(eth.toFixed(1)+"<span>ETH</span>");
					getEthPrice(eth);
				}
			};
			xmlhttp.open("GET", url, true);
			xmlhttp.send();
		}
		function getEthPrice(eth){
			var xmlhttp = new XMLHttpRequest();
			var url = "https://api.coinmarketcap.com/v2/ticker/1027/";
			
			xmlhttp.onreadystatechange = function() {
			if (this.readyState == 4 && this.status == 200) {
				var rs = JSON.parse(this.responseText);
					var price=rs.data.quotes.USD.price;
					var total_usd=eth*price;
					var percent=(total_usd*100)/10000000;
					if (percent<1) percent=1;
					$("#total_percent").css("width",percent+"%");
					$("#total_usd").html(total_usd.toFixed(2)+" usd");
				}
			};
			xmlhttp.open("GET", url, true);
			xmlhttp.send();
		}
		getTotalBalance();
	</script>
